<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.AddingMealController">
   <children>
      <AnchorPane prefHeight="280.0" prefWidth="243.0">
         <children>
            <Button layoutX="5" layoutY="125.0" mnemonicParsing="false" onAction="#addmeal" text="Vegetarian Meal" />
            <Label fx:id="lblMeal" alignment="CENTER" layoutY="44.0" prefHeight="21.0" prefWidth="243.0" textFill="RED" />
         </children>
         <children>
            <Button layoutX="5"  layoutY="200.0" mnemonicParsing="false" onAction="#getMeals" text="Show my number of vegetarian" />
            <Label fx:id="getMeals" alignment="CENTER" layoutY="66.0" prefHeight="50.0" prefWidth="243.0" textFill="RED" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
